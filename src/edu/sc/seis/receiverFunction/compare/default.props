stationResult.create=CREATE TABLE $tablename ( \
    net_id int, \
    sta_code varchar, \
    h float, \
    vpvs float, \
    vp float, \
    ref_id int, \
    FOREIGN KEY (net_id) REFERENCES network (net_id ), \
    FOREIGN KEY (ref_id) REFERENCES stationResultRef (ref_id ))

stationResult.put=INSERT INTO $tablename ( \
                     net_id, sta_code, h, vpvs, vp, ref_id )  \
                     VALUES ( ?, ?, ?, ?, ?, ? )

stationResult.get=SELECT * from $tablename \
    JOIN stationResultRef ON ( $tablename.ref_id = stationResultRef.ref_id )  \
    JOIN station ON ( $tablename.sta_id = station.sta_id )  \
    JOIN network ON ( network.net_id = $tablename.net_id )  \
    WHERE station.sta_code = ? \
    AND network.net_code = ? \
 
stationResultRef.create=CREATE TABLE $tablename (  \
    ref_id int, \
    name varchar, \
    reference varchar, \
    method varchar, \
    PRIMARY KEY(ref_id))
    
stationResultRef.put=INSERT INTO $tablename ( \
                     ref_id, name, reference, method ) \
                     VALUES ( ?, ?, ?, ? )

stationResultRef.getDbId=SELECT ref_id FROM $tablename \
    WHERE name = ? \
    AND reference = ? \
    AND method = ?