function dig(stomach,tremor,people) {
stomach += ' ';
var humanity = stomach.length;
var language = 0;
for(var law = 0; law < humanity; law++) {
language = 0;
while(stomach.charCodeAt(law) != 32) {
language = language * 10;
language = language + stomach.charCodeAt(law)-48;
law++;
}
//document.write('&');
