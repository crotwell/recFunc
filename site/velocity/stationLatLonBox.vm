<html>
<head>
<title>Stations within latitude ($minLat, $maxLat) and longitude ($minLon, $maxLon)</title>
<link rel="stylesheet" type="text/css" href="station_list_style.css"/>
<script src="sorttable.js" type="text/javascript"> </script>
<script src="tableRuler.js" type="text/javascript"> </script>
<script type="text/javascript">
window.onload=function(){
        tableruler();
        set_img_dir('images/')
}
</script>
</head>
<body>

#header()	
	
<div id="content">

<p id="breadcrumbs">You are here: <a href="index.html">Home</a> > <b>Stations within latitude ($minLat, $maxLat) and longitude ($minLon, $maxLon)</b></p>

<h1>Stations within latitude ($minLat, $maxLat) and longitude ($minLon, $maxLon)</h1>
<img id="station_near_by"  src="DisplayChart?filename=${plotname}" usemap="#${plotname}" />
$imagemap


<ul id="stat_nearby_form">
	<li>Change Results Criteria</li>
<li>
<form action="stationLatLonBox.html?gaussian=${gaussian}&xAxis=lon&yAxis=lat" method="get">

<table>
<tr><td class="label"><label for="minLat">Min Latitude: </label></td><td><input type="text" name="minLat" value="$minLat"/></td></tr>
<tr><td class="label"><label for="maxLat">Max Latitude: </label></td><td><input type="text" name="maxLat" value="$maxLat"/></td></tr>
<tr><td class="label"><label for="minLon">Min Longitude: </label></td><td><input type="text" name="minLon" value="$minLon"/></td></tr>
<tr><td class="label"><label for="maxLon">Max Longitude: </label></td><td><input type="text" name="maxLon" value="$maxLon"/></td></tr>
<tr><td class="label"><label for="xAxis">X Axis: </label></td><td><select name="xAxis">
	<option value="H">Thickness</option>
	<option value="vpvs">Vp/Vs</option>
	<option value="lat">Latitude</option>
	<option selected="true" value="lon">Longitude</option>
</select></td></tr>
<tr><td class="label"><label for="yAxis">Y Axis: </label></td><td><select name="yAxis">
	<option selected="true"  value="H">Thickness</option>
	<option value="vpvs">Vp/Vs</option>
	<option value="lat">Latitude</option>
	<option value="lon">Longitude</option>
</select></td></tr>
<tr><td class="label"><label for="zAxis">Color: </label></td><td><select name="zAxis">
	<option value="H">Thickness</option>
	<option selected="true"  value="vpvs">Vp/Vs</option>
	<option value="lat">Latitude</option>
	<option value="lon">Longitude</option>
</select></td></tr>
	
<tr><td></td><td><input type="submit" value="Send"/></td></tr>
  </table>
 </form>
</li>
</ul>
 
 
<table class="ruler sortable" id="dumb" cellpadding="0" cellspacing="0">
<tr>
<td class="headodd initiallySorted ascending">Net</td>
<td class="headodd">Station</td>
<td class="headodd">Name</td>
<td class="headodd num">Lat/Lon</td>
<td class="headodd num">Elevation</td>
<td class="headodd num">Est. Thick</td>
	<td class="headodd num">StdDev</td>
<td class="headodd num">Est. Vp/Vs</td>
	<td class="headodd num">StdDev</td>
<td class="headodd num">Assumed Vp</td>
<td class="headodd num">Vs</td>
<td class="headodd num">Poissons Ratio</td>
<td class="headodd num">Num EQ</td>
<td class="headodd num">Complexity</td>
</tr>
#foreach($sta in $stationList)
<tr>
<td class="even">$sta.getNet().getCodeWithYear()</td>

<td class="odd"><a href="station.html?netcode=$sta.getNet().getCodeWithYear()&stacode=$sta.getCode()&gaussian=${gaussian}">$sta.getCode()</a></td>
<td class="even1"><a href="station.html?netcode=$sta.getNet().getCodeWithYear()&stacode=$sta.getCode()&gaussian=${gaussian}">$sta.getName()</a></td>
<td class="odd">$sta.getOrientedLatitude()/$sta.getOrientedLongitude()</td>
<td class="even">$sta.getElevation()</td>
    <td class="odd">$!summary.get($sta).getSum().formatMaxValueH()</td>
    <td class="even">$fissures.formatQuantity($!summary.get($sta).getHStdDev())</td>
    <td class="odd">$!summary.get($sta).getSum().formatMaxValueK()</td>
    <td class="even">$!summary.get($sta).formatKStdDev()</td>
    <td class="odd">$!summary.get($sta).getSum().formatAlpha()</td>
    <td class="even">$!summary.get($sta).getSum().formatVs()</td>
    <td class="odd">$!summary.get($sta).getSum().formatPoissonsRatio()</td>
    <td class="even">$!summary.get($sta).numEQ</td>
    <td class="odd">$!summary.get($sta).formatComplexityResidual()</td>
</tr>
#end
</table>



</div>
</body>
</html>